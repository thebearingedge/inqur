{
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "include": [
    "api/**/*.js",
    "client/**/*.js"
  ],
  "exclude": [
    "**/test/**",
    "**/__test__.js",
    "**/__fixtures__.js",
    "**/*.test.js",
    "**/*.config.js",
    "**/index.js",
    "client/**/*.integration.js"
  ],
  "all": true
}
