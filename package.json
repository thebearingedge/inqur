{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "freeCodeCamp: Build a Voting App",
  "main": "build/server/",
  "scripts": {
    "db:make": "pg-bump create",
    "db:up": "pg-bump up",
    "db:down": "pg-bump down",
    "db:stat": "pg-bump status",
    "test:api": "mocha -r babel-register $(find api -name *.test.js ! -name *.data.test.js)",
    "tdd:api": "npm t -- -w -R min",
    "cover": "nyc npm test",
    "clean": "rimraf build/",
    "build": "babel api/ -d build/ --ignore test,__",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/voting-app.git"
  },
