{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "freeCodeCamp: Build a Voting App",
  "main": "build/",
  "scripts": {
    "db:make": "pg-bump create",
    "db:up": "pg-bump up",
    "db:down": "pg-bump down",
    "db:stat": "pg-bump status",
    "db:cycle": "run-s -s db:down db:up",
    "test:api": "mocha -r babel-register **/*.unit.test.js",
    "tdd:api": "run-s -s 'test:api -- -w -R min'",
    "test:db": "mocha -r babel-register **/*.data.test.js",
    "tdd:db": "run-s -s 'test:db -- -w -R min'",
    "test:server": "mocha -r babel-register api/**/*.integration.js",
    "tdd:server": "run-s -s 'test:server -- -w -R min'",
    "watch:server": "nodemon -x babel-node api",
    "watch:db": "onchange -iw 'db/**/*.sql' -- run-s -s db:cycle",
    "watch:client": "node client/",
    "build:client": "NODE_ENV=production next build client/",
    "start:client": "NODE_ENV=production node client/",
    "test:client": "mocha -r babel-register client/**/*.unit.test.js",
    "tdd:client": "run-s -s 'test:client -- -w -R min'",
    "dev": "run-p -s watch:*",
    "test": "run-p -s 'test:* -- -R mocha-silent-reporter'",
    "cover": "nyc npm t -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/voting-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebearingedge/voting-app/issues"
  },
  "homepage": "https://github.com/thebearingedge/voting-app#readme",
  "dependencies": {
    "axios": "0.16.2",
    "bcrypt": "1.0.2",
    "body-parser": "1.17.2",
    "camelcase": "4.1.0",
    "cors": "2.8.3",
    "deep-clone": "3.0.2",
    "dotenv": "4.0.0",
    "es6-error": "4.0.2",
    "express": "4.15.3",
    "joi": "10.5.2",
    "knex": "0.13.0",
    "next": "2.4.3",
    "qs": "6.4.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "snake-case": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-register": "6.24.1",
    "babili-webpack-plugin": "0.1.1",
    "chai": "4.0.2",
    "chai-struct": "3.0.0",
    "eslint": "3.19.0",
    "eslint-plugin-react": "7.0.1",
    "express-unit": "1.1.0",
    "faker": "4.1.0",
    "mocha": "3.4.2",
    "mocha-silent-reporter": "1.0.0",
    "moxios": "0.4.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.2",
    "onchange": "3.2.1",
    "pg-bump": "0.0.1",
    "redux-mock-store": "1.2.3",
    "rimraf": "2.6.1",
    "sinon": "2.3.4",
    "sinon-chai": "2.10.0",
    "webpack": "2.6.1"
  }
}
