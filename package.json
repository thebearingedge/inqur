  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "1.17.2",
    "camelcase": "4.1.0",
    "deep-clone": "3.0.2",
    "dotenv": "4.0.0",
    "es6-error": "4.0.2",
    "express": "4.15.3",
    "joi": "10.5.2",
    "knex": "0.13.0",
    "snake-case": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
