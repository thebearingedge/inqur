{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "freeCodeCamp: Build a Voting App",
  "main": "build/",
  "scripts": {
    "db:make": "pg-bump create",
    "db:up": "pg-bump up",
    "db:down": "pg-bump down",
    "db:stat": "pg-bump status",
    "db:cycle": "run-s -s db:down db:up",
    "db:watch": "onchange -iw 'db/**/*.sql' -- run-s -s db:cycle",
    "test:api": "mocha -r babel-register **/*.unit.test.js",
    "tdd:api": "run-s -s 'test:api -- -w -R min'",
    "test:db": "mocha -r babel-register **/*.data.test.js",
    "tdd:db": "run-s -s 'test:db -- -w -R min'",
    "build:server": "babel api/ -d build/ --ignore test,__",
    "watch:server": "run-s -s 'build:server -- -w'",
    "test": "run-s -s test:*",
    "tdd": "run-p -s tdd:*",
    "cover": "nyc run-p -s test:*",
    "clean": "rimraf build/",
    "start": "node .",
    "watch:start": "nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/voting-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebearingedge/voting-app/issues"
  },
  "homepage": "https://github.com/thebearingedge/voting-app#readme",
  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "1.17.2",
    "camelcase": "4.1.0",
    "deep-clone": "3.0.2",
    "dotenv": "4.0.0",
    "es6-error": "4.0.2",
    "express": "4.15.3",
    "joi": "10.5.2",
    "knex": "0.13.0",
    "snake-case": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-register": "6.24.1",
    "chai": "4.0.1",
    "chai-struct": "3.0.0",
    "eslint": "3.19.0",
    "express-unit": "1.1.0",
    "faker": "4.1.0",
    "mocha": "3.4.2",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.1",
    "onchange": "3.2.1",
    "pg-bump": "0.0.1",
    "rimraf": "2.6.1",
    "sinon": "2.3.2",
    "sinon-chai": "2.10.0"
  }
}
