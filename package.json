{
  "name": "inqur",
  "version": "1.0.0",
  "description": "A web app for pollsters who want the opinion of the internet.",
  "main": "build/",
  "scripts": {
    "db:make": "pg-bump create",
    "db:up": "pg-bump up",
    "db:down": "pg-bump down",
    "db:stat": "pg-bump status",
    "db:cycle": "run-s -s db:down db:up",
    "test:api:unit": "mocha -r babel-register api/**/*.unit.test.js",
    "tdd:api:unit": "run-s -s 'test:api:unit -- -w -R min'",
    "test:api:db": "mocha -r babel-register **/*.db.test.js",
    "tdd:api:db": "run-s -s 'test:api:db -- -w -R min'",
    "test:api:int": "mocha -r babel-register api/**/*.int.test.js",
    "tdd:api:int": "run-s -s 'test:api:int -- -w -R min'",
    "test:client:unit": "mocha -r babel-register client/**/*.unit.test.js",
    "tdd:client:unit": "run-s -s 'test:client:unit -- -w -R min'",
    "test:client:int": "mocha -r babel-register client/**/*.int.test.js",
    "tdd:client:int": "run-s -s 'test:client:int -- -w -R min'",
    "watch:api": "nodemon -x babel-node api",
    "watch:db": "onchange -iw 'db/**/*.sql' -- run-s -s db:cycle",
    "watch:client": "node client/",
    "dev": "run-p -s watch:*",
    "test": "run-p -s 'test:** -- -R mocha-silent-reporter'",
    "e2e:build": "rimraf client/.next && run-s -s db:cycle && next build client/",
    "e2e:test": "mocha -t 0 -r babel-register e2e/*.test.js",
    "e2e": "run-s -s e2e:build e2e:test",
    "cover": "nyc run-s -s test e2e",
    "lint": "eslint api/ client/ e2e/",
    "build:client": "NODE_ENV=production next build client/",
    "start:client": "NODE_ENV=production node client/",
    "precommit": "run-p -s lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/inqur.git"
  },
  "author": "Tim Davis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebearingedge/inqur/issues"
  },
  "homepage": "https://github.com/thebearingedge/inqur#readme",
  "dependencies": {
    "axios": "0.16.2",
    "bcrypt": "1.0.2",
    "body-parser": "1.17.2",
    "camelcase": "4.1.0",
    "cors": "2.8.3",
    "debounce": "1.0.2",
    "deep-clone": "3.0.2",
    "dotenv": "4.0.0",
    "es6-error": "4.0.2",
    "express": "4.15.3",
    "joi": "10.6.0",
    "jsonwebtoken": "7.4.1",
    "knex": "0.13.0",
    "next": "2.4.4",
    "next-redux-wrapper": "1.1.3",
    "pg": "6.4.0",
    "postgres-date": "1.0.3",
    "qs": "6.4.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "redis": "2.7.1",
    "redux": "3.7.0",
    "redux-form": "6.8.0",
    "redux-thunk": "2.2.0",
    "snake-case": "2.1.0",
    "styled-components": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-styled-components": "1.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.2",
    "chai-enzyme": "0.7.1",
    "chai-struct": "3.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.1.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-node": "5.1.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "express-unit": "1.1.0",
    "faker": "4.1.0",
    "husky": "0.14.1",
    "jsdom": "11.0.0",
    "mocha": "3.4.2",
    "mocha-silent-reporter": "1.0.0",
    "moxios": "0.4.0",
    "nightmare": "2.10.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.2",
    "onchange": "3.2.1",
    "pg-bump": "1.0.1",
    "react-test-renderer": "15.6.1",
    "redux-mock-store": "1.2.3",
    "rimraf": "2.6.1",
    "sinon": "2.3.5",
    "sinon-chai": "2.11.0",
    "webpack": "3.0.0"
  }
}
